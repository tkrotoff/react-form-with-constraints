{
  "name": "react-form-with-constraints-native",
  "version": "0.18.0",
  "description": "Simple form validation for React",
  "repository": "github:tkrotoff/react-form-with-constraints",
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "validation",
    "react-form-with-constraints",
    "react-native"
  ],
  "main": "lib-es5/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "lib-es5/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf lib lib-es5 coverage .rpt2_cache LICENSE",
    "tsc": "tsc",
    "build": "yarn build:esnext && yarn build:es5",
    "build:esnext": "tsc --project tsconfig.lib.json",
    "build:es5": "tsc --project tsconfig.lib-es5.json",
    "prepublishOnly": "yarn clean && yarn build",
    "prepack": "cp ../../LICENSE . && jscodeshift --transform=../../removeConsoleTransform.ts lib lib-es5",
    "postpack": "rm LICENSE",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react-form-with-constraints": ">=0.17.0",
    "react-native": ">=0.61.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/jscodeshift": "^0.7.1",
    "@types/react": "^16.9.53",
    "@types/react-native": "^0.63.25",
    "@types/react-test-renderer": "^16.9.3",
    "babel-jest": "^26.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest": "^26.6.0",
    "jscodeshift": "^0.11.0",
    "metro-react-native-babel-preset": "^0.63.0",
    "react": "^16.14.0",
    "react-form-with-constraints": "^0.18.0",
    "react-native": "^0.61.5",
    "react-test-renderer": "^16.14.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
